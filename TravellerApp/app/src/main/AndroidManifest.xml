<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="nl.nujules.travellerapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:replace="icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".DoLogin" />
        <activity android:name=".DoSignup" />
        <activity android:name=".Menu" />
        <activity android:name=".Placeholder" />
        <activity android:name=".Nearby" />
        <activity android:name=".NearbyResult" />
        <activity android:name=".NearbyResultSpecific" />
        <activity android:name=".AddReview"></activity>
    </application>

</manifest>
